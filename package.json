{
  "name": "kintone-plugin-age",
  "version": "0.1.0",
  "author": "ribbit",
  "license": "MIT",
  "scripts": {
    "develop": "webpack --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "watch": "webpack --config webpack.dev.js -w",
    "init": "run-s create-ppk move-ppk",
    "start": "run-p watch upload",
    "upload": "kintone-plugin-uploader dist/plugin-dev.zip --watch --waiting-dialog-ms 3000",
    "update-modules": "ncu -u",
    "check-updates": "ncu",
    "create-ppk": "kintone-plugin-packer --out dist/plugin-dev.zip plugin",
    "move-ppk": "node scripts/move-ppk.ts"
  },
  "devDependencies": {
    "@emotion/styled": "^11.3.0",
    "@kintone/dts-gen": "^5.0.3",
    "@kintone/plugin-packer": "^5.0.17",
    "@kintone/plugin-uploader": "^6.0.2",
    "@kintone/rest-api-client": "^2.0.19",
    "@kintone/webpack-plugin-kintone-plugin": "^5.0.17",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@types/react": "^17.0.21",
    "@types/react-dom": "^17.0.9",
    "babel-loader": "^8.2.2",
    "immer": "^9.0.6",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "terser-webpack-plugin": "^5.2.4",
    "ts-loader": "^9.2.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3",
    "utility-types": "^3.10.0",
    "webpack": "^5.52.1",
    "webpack-cli": "^4.8.0",
    "webpack-merge": "^5.8.0"
  },
  "private": true
}
